{
  "name": "testdriver-proxy",
  "version": "1.0.0",
  "description": "Proxy server to use TestDriver.ai with any LLM API (Z.ai, OpenAI, Anthropic)",
  "main": "server.js",
  "bin": {
    "testui": "./bin/testui",
    "context": "./bin/context"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "./tests/scripts/run_tests.sh",
    "test:live": "./tests/scripts/run_live_tests.sh",
    "validate": "./start-validation.sh",
    "deploy": "./deploy-and-test.sh",
    "execute": "./execute-test.sh",
    "docker:build": "docker build -t testdriver-proxy .",
    "docker:run": "docker run -p 8080:8080 --env-file .env testdriver-proxy",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f"
  },
  "keywords": [
    "testdriver",
    "ai",
    "testing",
    "automation",
    "llm",
    "proxy",
    "z.ai",
    "openai",
    "anthropic",
    "vision",
    "ui-testing"
  ],
  "author": "TestDriver.ai Community",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "js-yaml": "^4.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "selenium-webdriver": "^4.16.0",
    "sharp": "^0.34.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zeeeepa/cli.git",
    "directory": "testdriver-proxy"
  },
  "bugs": {
    "url": "https://github.com/Zeeeepa/cli/issues"
  },
  "homepage": "https://github.com/Zeeeepa/cli/tree/main/testdriver-proxy#readme"
}
