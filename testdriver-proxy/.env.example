# ============================================================================
# TestDriver Proxy Configuration
# ============================================================================

# API Provider Configuration
# Choose: 'zai', 'anthropic', 'openai'
API_PROVIDER=anthropic

# API Credentials (choose based on provider)
API_KEY=your_api_key_here

# API Base URL
# For Z.ai with Anthropic format: https://api.z.ai/api/anthropic
# For OpenAI: https://api.openai.com/v1
API_BASE_URL=https://api.z.ai/api/anthropic

# Model Configuration
# For Z.ai: glm-4v-plus, glm-4.5v
# For Anthropic: claude-3-sonnet-20240229
# For OpenAI: gpt-4-vision-preview
MODEL=glm-4.5v

# Server Configuration
PORT=3000
DEBUG=false

# Performance Tuning
MAX_TOKENS=4000
TEMPERATURE=0.7
TIMEOUT=120000

# Rate Limiting (requests per 15 minutes per IP)
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Session Management  
TESTDRIVER_PROXY_SESSION_TIMEOUT_MINUTES=30
TESTDRIVER_PROXY_MAX_SESSIONS=1000
